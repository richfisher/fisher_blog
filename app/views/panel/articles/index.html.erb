<h1>Listing articles</h1>
<%= link_to 'New Article', new_panel_article_path %>

<table class="table">
  <tr>
    <th>title</th>
    <th></th>
    <th></th>
  </tr>

<% @articles.each do |article| %>
  <tr>
    <td><%= article.title %></td>
    <td><%= link_to 'Edit', edit_panel_article_path(article) %></td>
    <td><%= link_to 'Destroy', [:panel, article], method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

